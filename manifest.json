{
  "manifest_version": 2,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Manage your browser tabs efficiently",
  "permissions": [
    "tabs",
    "storage",
    "tabGroups",
    "windows",
    "chrome://favicon/"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "minimum_edge_version": "79.0.309.0",
  "web_accessible_resources": [
    "manager.html",
    "manager.js",
    "manager.css",
    "icons/*"
  ]
}